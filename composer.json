{
    "name": "vanio/vanio-domain-bundle",
    "description": "Symfony2 Bundle providing some additional features suitable when programming your domain.",
    "keywords": ["ddd", "domain driven", "doctrine"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/vaniocz/vanio-domain-bundle",
    "license": "MIT",
    "authors": [
        {"name": "Marek Štípek", "email": "marek.stipek@vanio.cz"}
    ],
    "repositories": [{
        "url": "https://github.com/maryo/JsFormValidatorBundle.git",
        "type": "git"
    }, {
        "url": "https://github.com/dizzy7/DoctrineFullTextPostrgres.git",
        "type": "git"
    }],
    "require": {
        "php": "7.0.*",
        "beberlei/assert": "^2.6.5",
        "ramsey/uuid-doctrine": "^1.2",
        "happyr/doctrine-specification": "^0.8@dev",
        "sensio/framework-extra-bundle": "^3.0",
        "symfony/form": "^3.0",
        "symfony/validator": "^3.0",
        "vanio/stdlib": "^0.1@dev",
        "vanio/type-parser": "^0.1@dev",
        "vanio/doctrine-domain-events": "^0.1@dev",
        "vanio/doctrine-generic-types": "^0.1@dev",
        "vertigolabs/doctrine-full-text-postgres": "dev-master#75e767c660fcbc85fa29db40946242f5172a5721"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "vich/uploader-bundle": "^1.3.1",
        "fp/jsformvalidator-bundle": "dev-fork",
        "matthiasnoback/symfony-dependency-injection-test": "^1.0",
        "vanio/coding-standards": "^0.1@dev"
    },
    "autoload": {
        "psr-4": {"Vanio\\DomainBundle\\": ""},
        "exclude-from-classmap": ["/Tests/", "**/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {"Vanio\\DomainBundle\\Tests\\": "Tests/"}
    },
    "suggest": {
    },
    "scripts": {
        "test": "\"vendor/bin/phpunit\"",
        "lint": "\"vendor/bin/phpcs\" --standard=vendor/vanio/coding-standards/ruleset.xml --extensions=php --encoding=utf-8 --ignore=vendor .",
        "fix": "\"vendor/bin/phpcbf\" --standard=vendor/vanio/coding-standards/ruleset.xml --extensions=php --encoding=utf-8 --no-patch --ignore=vendor ."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
